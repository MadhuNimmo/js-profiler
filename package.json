{
  "name": "js-profiler",
  "version": "2.0.0",
  "description": "Javascript profiling tool and collection of performance profiles for various JavaScript built-ins.",
  "main": "./lib/index.js",
  "man": "./docs/js-profiler.1.gz",
  "bin": {
    "js-profiler": "./js-profiler.js"
  },
  "scripts": {
    "start": "node js-profiler.js",
    "test": "mocha --recursive \"./{,!(node_modules)/**/}*.spec.js\"",
    "tdd": "mocha --recursive --watch \"./{,!(node_modules)/**/}*.spec.js\"",
    "lint": "eslint -c ./.eslintrc.json **/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run lint && npm test"
    }
  },
  "keywords": [
    "Performance",
    "JavaScript",
    "Built-Ins"
  ],
  "author": {
    "name": "Hans-Peter Dietz",
    "email": "h.p.dietz@gmail.com",
    "twitter": "@h_p_d",
    "url": "https://hpdietz.com"
  },
  "contributors": [
    {
      "name": "Hans-Peter Dietz",
      "email": "h.p.dietz@gmail.com",
      "twitter": "@h_p_d",
      "url": "https://hpdietz.com"
    },
    {
      "name": "Fabian Maschewski",
      "url": "https://github.com/f-masche"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "glob": "^7.1.4",
    "node-getopt": "^0.2.4"
  },
  "devDependencies": {
    "@haensl/eslint-config": "^1.3.0",
    "chai": "^3.5.0",
    "eslint": "^4.19.1",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.14.0"
  },
  "engines": {
    "node": ">=8.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haensl/js-profiler.git"
  },
  "bugs": {
